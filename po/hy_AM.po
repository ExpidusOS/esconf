#: ../esconf/esconf-cache.c:979
#, c-format
msgid "Failed to make ResetProperty DBus call"
msgstr "Չյաջողուեց ստեղծել ResetProperty DBus"

#: ../esconfd/esconf-backend-perchannel-xml.c:276
#, c-format
msgid "Unable to create configuration directory \"%s\""
msgstr "Հնարաւոր չէ ստեղծել կազմաձեւման «%s» նշարան"

#: ../esconfd/esconf-backend-perchannel-xml.c:321
#: ../esconfd/esconf-daemon.c:167
#, c-format
msgid "Permission denied while modifying property \"%s\" on channel \"%s\""
msgstr "Թոյլտուութիւնը մերժուել է «%s» ալիքում «%s» յատկութեան փոփոխման ժամանակ"

#: ../esconfd/esconf-backend-perchannel-xml.c:386
#: ../esconfd/esconf-backend-perchannel-xml.c:468
#: ../esconfd/esconf-backend-perchannel-xml.c:633
#: ../esconfd/esconf-backend-perchannel-xml.c:653 ../esconf-query/main.c:344
#, c-format
msgid "Property \"%s\" does not exist on channel \"%s\""
msgstr "«%s» յատկութիւնը գոյութիւն չունի «%s» ալիքում"

#: ../esconfd/esconf-backend-perchannel-xml.c:600
#, c-format
msgid "Unable to remove channel \"%s\": %s"
msgstr "Հնարաւոր չէ հեռացնել «%s» ալիքը․ %s"

#: ../esconfd/esconf-backend-perchannel-xml.c:1297
#, c-format
msgid "Invalid type for <property>: \"%s\""
msgstr "Անվաւեր տիպ <property>֊ի համար․ «%s»"

#: ../esconfd/esconf-backend-perchannel-xml.c:1313
#: ../esconfd/esconf-backend-perchannel-xml.c:1389
#, c-format
msgid "Unable to parse value of type \"%s\" from \"%s\""
msgstr "Հնարաւոր չէ վերլուծել «%s»֊ի արժեքը «%s»֊ից"

#: ../esconfd/esconf-backend-perchannel-xml.c:1369
#, c-format
msgid "The type attribute of <value> cannot be an array"
msgstr "<value>֊ի յատկանիշի տիպը չի կարող զանգուած լինել"

#: ../esconfd/esconf-backend-perchannel-xml.c:1378
#, c-format
msgid "Invalid type for <value>: \"%s\""
msgstr "Անվաւեր տիպ <value>֊ի համար․ «%s»"

#: ../esconfd/esconf-backend-perchannel-xml.c:1654
#: ../esconfd/esconf-backend-perchannel-xml.c:1903
#, c-format
msgid "Channel \"%s\" does not exist"
msgstr "«%s» ալիքը գոյութիւն չունի"

#: ../esconfd/esconf-backend-perchannel-xml.c:1961
#, c-format
msgid "Unable to write channel \"%s\": %s"
msgstr "Հնարաւոր չէ գրառել «%s» ալիքը․ %s"

#: ../esconfd/esconf-backend-factory.c:77
#, c-format
msgid "Unable to find Esconf backend of type \"%s\""
msgstr "Հնարաւոր չէ գտնել Esconf֊ի «%s» տիպի ետնաբեմը"

#: ../esconfd/esconf-backend.c:101
#, c-format
msgid "Property names must start with a '/' character"
msgstr "Յատկութեան անունը պէտք է սկսուի «/» գրանշանով"

#: ../esconfd/esconf-backend.c:110
#, c-format
msgid "The root element ('/') is not a valid property name"
msgstr "Արմատային տարրը («/») յատկութեան անվաւեր անուն է"

#: ../esconfd/esconf-backend.c:125
#, c-format
msgid ""
"Property names can only include the ASCII characters A-Z, a-z, 0-9, '_', "
"'-', ':', '.', ',', '[', ']', '{', '}', '<' and '>', as well as '/' as a "
"separator"
msgstr "Յատկության անունները կարող են պարունակել, միայն ASCII-գրանշաններ A-Z, a-z, 0-9, «_», «-», «:», «.», «,», «[», «]», «{», «}», «<» եւ «>», ինչպէս նաեւ «/»֊ը, որպէս բաժանիչ"

#: ../esconfd/esconf-backend.c:134
#, c-format
msgid "Property names cannot have two or more consecutive '/' characters"
msgstr "Յատկութեան անունները չեն կարող ունենալ երկու կամ աւելի «/» գրանշաններ"

#: ../esconfd/esconf-backend.c:145
#, c-format
msgid "Property names cannot end with a '/' character"
msgstr "Յատկութեան անունները չեն կարող աւարտուել «/» գրանշանով"

#: ../esconfd/esconf-backend.c:162
#, c-format
msgid "Channel name cannot be an empty string"
msgstr "Ալիքի անուան դաշտը չի կարող դատարկ լինել"

#: ../esconfd/esconf-backend.c:177
#, c-format
msgid ""
"Channel names can only include the ASCII characters A-Z, a-z, 0-9, '{', '}',"
" '|', ']', '[', ':', ',', '.', '_', and '-'"
msgstr "Ալիքի անունները կարող են պարունակել միայն ASCII գրանշաններ․ A-Z, a-z, 0-9, «{», «}», «|», «]», «[», «:», «,», «.», «_» եւ «-»"

#: ../esconfd/esconf-backend.c:399
#, c-format
msgid ""
"The property name can only be empty or \"/\" if a recursive reset was "
"specified"
msgstr "Յատկութեան անունը կարող է լինել դատարկ կամ \"/\" եթե նշուի ինքնադարձուն վերատեղադրումը"

#: ../esconfd/main.c:125
#, c-format
msgid "Name %s lost on the message dbus, exiting."
msgstr "Անուն %s ֊ը կորել է dbus հաղորդագրութիւնում, ելք։"

#: ../esconfd/main.c:148
msgid "Prints the esconfd version."
msgstr "Ցուցադրել Esconf֊ի տարբերակը։"

#: ../esconfd/main.c:150
msgid ""
"Configuration backends to use.  The first backend specified is opened "
"read/write; the others, read-only."
msgstr "Կազմաձեւման աւգտագործվող ետնաբեմերը։ Սահմանված առաջին ետնաբեմը բացվել է կարդալ/գրելու, մնացածը՝ սոսկ ընթեռնելի տարբերակով։"

#: ../esconfd/main.c:153
msgid "Fork into background after starting; only useful for testing purposes"
msgstr "Անցնել յետնաշերտային եղանակին մեկնարկից յետոյ՝ աւգտատակար է միայն փորձարկային նպատակների դէպքում"

#: ../esconfd/main.c:173
msgid "Expidus Configuration Daemon"
msgstr "Expidus֊ի Կազմաձեւման Աւժանդակ Ծրագիր"

#: ../esconfd/main.c:182
msgid "Expidus configuration daemon"
msgstr "Expidus֊ի կազմաձեւման աւժանդակ ծրագիր"

#: ../esconfd/main.c:184
msgid "Report bugs to http://bugs.expidus.org/\n"
msgstr "Վրիպակների դէպքում կարող էք դիմել http://bugs.expidus.org/ հասցէով\n"

#: ../esconfd/main.c:187
#, c-format
msgid "Error parsing options: %s\n"
msgstr "Սխալի վերլուծման ընտրանքներ․ %s\n"

#: ../esconfd/esconf-daemon.c:222
#, c-format
msgid "GType transformation failed \"%s\""
msgstr "GType փոխակերպումը չյաջողուեց «%s»"

#: ../esconfd/esconf-daemon.c:483
#, c-format
msgid "No backends could be started"
msgstr "Ոչ մի յետնաբեմ չի գործարկուել"

#: ../esconf-query/main.c:90 ../esconf-query/main.c:95
msgid "set"
msgstr "տեղադրել"

#: ../esconf-query/main.c:100
msgid "reset"
msgstr "վերատեղադրել"

#: ../esconf-query/main.c:176
msgid "Version information"
msgstr "Տարբերակի վերաբերեալ տեղեկոյթ"

#: ../esconf-query/main.c:180
msgid "The channel to query/modify"
msgstr "Ալիք՝ հարցման/փոփոխման համար"

#: ../esconf-query/main.c:184
msgid "The property to query/modify"
msgstr "Յատկութիւն՝ հարցման/փոփոխման համար"

#: ../esconf-query/main.c:188
msgid "The new value to set for the property"
msgstr "Նոր արժէք, յատկութեան սահմանման համար"

#: ../esconf-query/main.c:192
msgid "List properties (or channels if -c is not specified)"
msgstr "Ցուցակի յատկութիւններ (կամ ալիքներ, եթե -c֊ն նշուած չէ)"

#: ../esconf-query/main.c:196
msgid "Verbose output"
msgstr "Մանրամասն արտածում"

#: ../esconf-query/main.c:200
msgid "Create a new property if it does not already exist"
msgstr "Ստեղծել նոր յատկութիւն, եթե այն արդէն գոյութիւն չունի"

#: ../esconf-query/main.c:204
msgid "Specify the property value type"
msgstr "Նշել յատկութեան արժէքի տիպը"

#: ../esconf-query/main.c:208
msgid "Reset property"
msgstr "Վերատեղադրել յատկութիւնը"

#: ../esconf-query/main.c:212
msgid "Recursive (use with -r)"
msgstr "Ինքնադարձուն (աւգտագործուում է -r֊ով)"

#: ../esconf-query/main.c:216
msgid "Force array even if only one element"
msgstr "Աւգտագործել զանգուածը, նոյնիսկ մեկ տարրի առկայութեան դէպքում"

#: ../esconf-query/main.c:220
msgid "Invert an existing boolean property"
msgstr "Հակադարձել գոյութիւն ունեցող տրամաբանական յատկութիւնը"

#: ../esconf-query/main.c:224
msgid "Monitor a channel for property changes"
msgstr "Վերահսկել ալիքը յատկութիւնների փոփոխման համար"

#: ../esconf-query/main.c:249
#, c-format
msgid "Failed to init libesconf: %s"
msgstr "Չյաջողուեց սկզբնաւորել libesconf֊ը․ %s"

#: ../esconf-query/main.c:254
msgid "- Esconf commandline utility"
msgstr "- Esconf հրամանային տողի աւժանդակ ծրագիր"

#: ../esconf-query/main.c:259
#, c-format
msgid "Option parsing failed: %s"
msgstr "Ընտրանքի վերլուծման ձախողում․ %s"

#: ../esconf-query/main.c:269
msgid "The Expidus development team. All rights reserved."
msgstr "Expidus֊ի մշակման խումբ։ Բոլոր իրաւունքները պաշտպանուած են։"

#: ../esconf-query/main.c:270
#, c-format
msgid "Please report bugs to <%s>."
msgstr "Սխալների մասին կարող էք զեկուցել <%s> հասցէով։"

#: ../esconf-query/main.c:282
msgid "Channels:"
msgstr "Ալիքներ․"

#: ../esconf-query/main.c:298
msgid "No property specified"
msgstr "Յատկութիւնը սահմանուած չէ"

#: ../esconf-query/main.c:304
msgid "--create and --reset options can not be used together"
msgstr "--create եւ --reset ընտրանքները չեն կարող գործածուել միասին"

#: ../esconf-query/main.c:310
msgid "--create and --reset options can not be used together with --list"
msgstr "--create եւ --reset ընտրանքները չեն կարող աւգտագործուել --list֊ի հետ միասին"

#: ../esconf-query/main.c:322
#, c-format
msgid "Start monitoring channel \"%s\":"
msgstr "Սկսել «%s» ալիքի վերահսկումը․"

#: ../esconf-query/main.c:356 ../esconf-query/main.c:461
#: ../esconf-query/main.c:530
msgid "Failed to set property"
msgstr "Չյաջողուեց տեղադրել յատկութիւնը"

#: ../esconf-query/main.c:360
msgid "--toggle only works with boolean values"
msgstr "--toggle֊ն աշխատում է միայն տրամաբանական արժէքների հետ"

#: ../esconf-query/main.c:369 ../esconf-query/main.c:388
msgid "(unknown)"
msgstr "(անյայտ)"

#: ../esconf-query/main.c:378
#, c-format
msgid "Value is an array with %d items:"
msgstr "Զանգվածը %d տարրերի հետ միասին արժեք է․"

#: ../esconf-query/main.c:402
#, c-format
msgid ""
"Property \"%s\" does not exist on channel \"%s\". If a new property should "
"be created, use the --create option"
msgstr "«%s» յատկութիւնը գոյութիւն չունի «%s» ալիքում։ Նոր յատկութիւն ստեղծելու համար աւգտագործէք --create ընտրանքը"

#: ../esconf-query/main.c:410
msgid "When creating a new property, the value type must be specified"
msgstr "Երբ ստեղծուում է նոր յատկութիւն, արժէքի տիպը պէտք է սահմանուած լինի"

#: ../esconf-query/main.c:420
msgid "Failed to get the existing type for the value"
msgstr "Չյաջողուեց ստանալ գոյութիւն ունեցող տիպը՝ արժէքի համար"

#: ../esconf-query/main.c:438
msgid "Unable to determine the type of the value"
msgstr "Հնարաւոր չէ որոշել արժէքի տիպը"

#: ../esconf-query/main.c:444
msgid "A value type must be specified to change an array into a single value"
msgstr "Արժեքի տիպը պէտք է սահմանուած լինի, զանգուածը մեկ արժէքի փոխելու համար"

#: ../esconf-query/main.c:454 ../esconf-query/main.c:515
#, c-format
msgid "Unable to convert \"%s\" to type \"%s\""
msgstr "Հնարաւոր չէ փոխարկել «%s»֊ը «%s» տիպի"

#: ../esconf-query/main.c:486
#, c-format
msgid "There are %d new values, but only %d types could be determined"
msgstr "Կան %d նոր արժէքներ, բայց միայն %d տիպերը կարող են որոշուել"

#: ../esconf-query/main.c:507
#, c-format
msgid "Unable to determine type of value at index %d"
msgstr "Հնարաւոր չէ որոշել արժէքի տիպը %d դասիչում"

#: ../esconf-query/main.c:557
#, c-format
msgid "Channel \"%s\" contains no properties"
msgstr "«%s» ալիքը չի պարունակում յատկութիւններ"
